   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="shawi-att.global-xml-lang-on-object-lang-elts-constraint-rule-9">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              context="tei:entry|tei:gram[@type=('arguments','collocate','diaRoot','government','morphPattern','synRoot')]|tei:orth|tei:quote">
         <s:assert test="exists(@xml:lang)">Elements containing object language must have an @xml:lang attribute.</s:assert>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="shawi-att.global-order-of-langs-constraint-rule-10">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              context="tei:cit[@type='example']">
         <s:assert test="not(@xml:lang)">cit must not have @xml:lang</s:assert>
      </s:rule>
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              context="tei:orth|tei:quote">
         <s:assert test="@xml:lang">Needs to have a lang attribute</s:assert>
      </s:rule>
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              context="tei:form[@type][tei:orth[@xml:lang='ar-Latn'] and tei:orth[@xml:lang='ar']]">
         <s:assert test="tei:orth[@xml:lang='ar-Latn'][following-sibling::tei:orth[@xml:lang='ar']]">ar-Latn has to be before ar</s:assert>
      </s:rule>
   </pattern>
