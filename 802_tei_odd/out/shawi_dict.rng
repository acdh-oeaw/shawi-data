   <define name="seg">
      <element name="seg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(arbitrary segment) represents any segmentation of text below the chunk level. [17.3. Blocks, Segments, and Anchors 6.2. Components of the Verse Line 7.2.5. Speech Contents]</a:documentation>
         <ref name="macro.lexicalParaContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="shawi-seg-seg-context-constraint-rule-58">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:seg[@type = 'hint']">
               <s:assert test="parent::tei:quote[parent::tei:cit[@type = 'translation']]">a
                              hint segment must be inside a translation equivalent</s:assert>
            </s:rule>
         </pattern>
         <ref name="att.global.attributes"/>
         <ref name="att.cmc.attributes"/>
         <ref name="att.notated.attributes"/>
         <ref name="att.segLike.attributes"/>
         <ref name="att.typed.attribute.subtype"/>
         <ref name="att.written.attributes"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>hint</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a hint in a translation to narrow down the semantic scope of the particular item.</a:documentation>
               <value>constrPart</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">constrPart is used in constructions</a:documentation>
               <value>oRef</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">TODO add description ???</a:documentation>
               <value>exPart</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">exPart stands for example part and is used to encode tokens in example sentences.</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
