   <define name="listBibl">
      <element name="listBibl">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(citation list) contains a list of bibliographic citations of any kind. [3.12.1. Methods of Encoding Bibliographic References and Lists of References 2.2.7. The Source Description 16.3.2. Declarable Elements]</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="bibl"/>
               <ref name="biblStruct"/>
            </choice>
         </oneOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="shawi-listBibl-literatureCheck-constraint-rule-25">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:bibl/tei:title[@ref='zot:Bettini2006']">
               <s:assert test="number(substring-after(./following-sibling::tei:biblScope, 'p.')) &gt; 358                                         and                                         number(substring-after(./following-sibling::tei:biblScope, 'p.')) &lt; 399">If Bettini2006 is the source, the following biblScope pages must be between 359 and 398.</s:assert>
            </s:rule>
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:bibl/tei:title[@ref='zot:Lentin2013']">
               <s:assert test="number(substring-after(./following-sibling::tei:biblScope, 'p.')) &gt; 150                                         and                                         number(substring-after(./following-sibling::tei:biblScope, 'p.')) &lt; 172">If Lentin2013 is the source, the following biblScope pages must be between 151 and 171.</s:assert>
            </s:rule>
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:bibl/tei:title[@ref='zot:Younes2021']">
               <s:assert test="number(substring-after(./following-sibling::tei:biblScope, 'p.')) &gt; 0                                         and                                         number(substring-after(./following-sibling::tei:biblScope, 'p.')) &lt; 137">If Younes2021 is the source, the following biblScope pages must be between 1 and 136.</s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="shawi-listBibl-listBibl-context-constraint-rule-28">
            <s:rule xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:listBibl[ancestor::tei:body]">
               <s:assert test="parent::tei:entry or parent::tei:cit[@type = 'example']">a listBibl is expected to be a direct child of an entry or an
                              example</s:assert>
               <s:assert test="not(tei:biblStruct)">a listBibl inside of an entry of an
                              example is composed of bibl elements</s:assert>
            </s:rule>
         </pattern>
         <ref name="att.global.attributes"/>
         <ref name="att.cmc.attributes"/>
         <ref name="att.declarable.attributes"/>
         <ref name="att.sortable.attributes"/>
         <ref name="att.typed.attribute.subtype"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>dictionaries</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">the listBibl is of type dictionaries</a:documentation>
               <value>corpora</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">the listBibl is of type corpora</a:documentation>
               <value>literature</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">the listBibl is of type literature</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
