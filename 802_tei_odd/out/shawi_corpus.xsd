                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction>
                        <xs:simpleType>
                           <xs:list>
                              <xs:simpleType>
                                 <xs:restriction base="xs:anyURI">
                                    <xs:pattern value="\S+"/>
                                 </xs:restriction>
                              </xs:simpleType>
                           </xs:list>
                        </xs:simpleType>
                        <xs:minLength value="1"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
