        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>Apo ʕid-hum hināk, Abdulla Öcalan ysawwūn besleme hināk, ʕugub Turkiyye ziʕlat ʕalē-hum gālaw</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
