        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24"
                TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE>ʕēš al-ᵊmbarrad ʕād, al-ᵊmḥayyir al-yabanǧi yōm-inn-u ta-ngūl yiǧi ysūg ʕala bint-i w yiǧi yṣīr ᵊḥyār,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
