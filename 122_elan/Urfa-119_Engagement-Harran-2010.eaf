        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25"
                TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>ta-ngūl ʕala bint-i yṣīr ᵊḥyār tā mā-yṣīr kōn, al-aǧnabi ʕād al mū-hu gaṛābit-ne, aṣ-ṣāyiǧ ygūl: </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26"
                TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE>“xayyo, tamām, xayyo ʕalē-kum ᵊḥyār iḥne linne ʕēš al-ᵊmbarrad, iḥne ʕēš al-ḥārr mā nəkl-u, mā niʕliǧ iṯum-ne, ʕalēš tā nitikāwan?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27"
                TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE>ʕalēš tā nihni linne ʕēš al-ᵊmbarrad, ʕēš al-ᵊmbarrad?” yaʕni ygūl “iḥne b-qēr kōn, ᵊb-qēr ᵊhniyye.”</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
