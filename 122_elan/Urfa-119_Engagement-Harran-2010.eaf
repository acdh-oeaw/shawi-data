        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>mā-yxallōn-he ittiǧawwaz ygūl “hāḏi mḥayyaṛa ˁaliyye liyye mānṭī-he, xall-ha arīd amawwit-he b-bēt-he, ˀamawwit b-dāṛ-ha ˁind umm-ha. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24"
                TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE>ˁēš al-ᵊmbarrad ˁād, ˀal-ᵊmḥayyir al-yabanǧi yōm-inn-u ta-ngūl yiǧi yṣūg ˁala bint-i w-yiǧi yṣīr ᵊḥyār,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25"
                TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>ta-ngūl ˁala bint-i yṣīr ᵊḥyār ta-mā-yṣīr kōn, ˀal-aǧnabi ˁād al mūhu garrābit-ne, ˀaṣ-ṣāyiǧ ygūl: </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26"
                TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE>“xayyo, tamām, xayyo ˁalē-kum ᵊḥyār iḥne linne ˁēš al-ᵊmbarrad, iḥne ˁēš al-ḥārr mā-nāklu, mā-niˁliǧ iṯum-ne, ˁalēš ta-nitkāwan?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27"
                TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE>ˁalēš ta-nihni linne ˁēš al-ᵊmbarrad, ˁēš al-ᵊmbarrad?” yaˁni ygūl “iḥne b-qēr kōn, ᵊb-qēr ᵊhniyye.”</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
