        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>ḥaḏ̣rati ʕAli, raḏ̣iya llāhu ʕan-h, nahāṛ yrūḥ ʕala ḥarib, ḥarib yḏ̣all ušhur ʕašər… čam yōm xamasṭaʕaš yōm ʕišrīn yōm yḏ̣allūn ᵊb-ḥarb.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>w yruddūn m-al-ḥarib yiǧi ʕala bēt-u, yōm-in yiǧi ʕala bēt-u, ǟǟǟ umm-ne Fāṭme, rāʕit bēt-u, ḥaḏ̣rati Fāṭme.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>ᵊtgūl-l-u: yā ʕAli! al-Ḥasan w-al-ᵊḤsēn wēlād-u, al-Ḥasan w-al-ᵊḤsēn ʕumur-hum sitte sabʕ ᵊsnīn. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>ᵊtgūl-l-u: al-Ḥasan w-al-ᵊḤsēn ǧūʕānīn, māmiš akil, māmiš ačil tā yāklūn-u, mā yāklūn ačil, ǧūʕānīn.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>ḥaḏ̣rati ʕAli ydaḥḥiǧ ygūl: yā ṣabūr yā aḷḷa! ilāh-i mawlā. yā rabb-i! mā ʕind-u flūs, məṣāri mā ʕind-u, məṣāri.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>ydaḥḥiǧ hīčiḏ yilbas al-yelege, ǟǟǟ çelik zırh al yilbas-he b-al-ḥarib, iḥne ngūl-he b-at-turuk çelik zırh.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>hiyye hnīt al-yelege ḥadīd, yilbas-he tā as-syūf mā tluxx-u yōm-in ykāwin.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>gāyil: yā rabb-i! yā ṣabūr yā aḷḷa, ad-arūḥ ʕa-s-sūg w d-abīʕ hāḏi. abīḥ-ḥe, w-aštari il-ʕiǧyān-i, l-al-Ḥasan w-al-ᵊḤsēn aǧīb-il-hum ačil. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>o zaman, ʕala ḏīč as-sāʕa zimān ačil aš-šī šinhi ta-ngūl? xubuz bəṭāṭa aw ačil aw laḥam, tā yāxḏ-ul-hum ᵊb-hāḏi ybīʕ al-ḥadīde tā yǧīb-il-hum ačil.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>rāyiḥ ʕa-s-sūg gāyim ᵊmdawwir ᵊmdawwir bāyiḥ-ḥe b-xamsimye dirham. awwali dirham šī, ygūlūn dirham.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>yōm-in bāyiḥ-ḥe b-xamsimye dirham, māxḏ al-məṣāri, ad-dirham, w ǧāy tā yrīd yištiri sabze il-ʕiǧyān-u, sabzawāt ta-ngūl b-as-s… miṯil alḥaz.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>gabəḷ-ma yiṣal as-sūg, b-as-sūg iṯnēne ǧaʕad ytikāwanūn, ᵊzlimtēn. in-hum iṯnēn ǧaʕad ytikāwanūn.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>hāḏa ǧāy ᵊmdaḥḥiǧ gāyil: ʕalēš ǧaʕad titikāwanūn bābo intu? wāḥad min-hum gāyil, mā yʕarfūn ḥaḏ̣reti ʕAli,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>gāyil: yā xayy-i! āni, aṭlub az-zlime hāḏa xamsimye dirham məṣāri flūs lī-´ ʕalē-´. w ǧaʕad arīd-hin minn-u mā yinṭī-ni.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>tss, ḥaḏ̣reti ʕAli gāyil: yā ṣabūr yā allāh! w rādd ʕala ḏāk: xayyo, az-zlime hāḏa yuṭulb-ak xamsimye dirham?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16"
                TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>gāyil: naʕam yuṭlub-ni, yā ṣadīg-i. gāyil: ʕalē mā tənṭī-´? gāyil: mā ʕind-i. w ygūl: alḥaz yā tinṭī-ni yā-hin yā aḏbaḥ-ak.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17"
                TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>tss, gāyil: lā, bābo! lā tiḏbaḥ-u! āni miššān xāṭir aḷḷa, hā-k xayyo! midd īd-u ʕala ǧēb-ak uṭluʕ al-xamsimye dirham. hā-k xayyo xamsimye dirham-ak!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>w-āni miššān aḷḷa d-afukk šarr-kum, tā mā ttikāwanūn. minṭi al-xamsimye dirham luwwa w rāyiḥ.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>ǧāy ḥaḏ̣reti ʕAli ʕa-l-bēt. ad-dirham rāḥ, al-məṣāri rāḥ, mā ǧāb šī il-ʕiǧyān-u ḥaḏ̣reti ʕAli w-al-ᵊḤsēn uxṛa,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>ǧāʕid b-al-bēt hīčiḏ, haḏōle maʕmaʕ ytimaʕmaʕūn ǧūʕānīn, yrīdūn ačil.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>gāyilt-ill-u umm-ne Fāṭme: yā ʕAli! ʕalēš mā ǧibit ačil? yā, gāyil: yā Fāṭme! al-ḥāl mesela hīčiḏ w kaḏa w kaḏa riḥit ʕa-s-sūg, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22"
                TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>w biʕit hāḏi al-yelege b-xamsimye dirham. w ǧīt ʕa-s-sūg w ǧaʕad aǧī tā_rūḥ ʕa-s-sūg, w šift iṯnēne ǧaʕad ytikāwanūn. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>miššān xāṭir aḷḷa fakkēt kōn-hum ī wāḥad yuṭlub wāḥad xamsimye dirham.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24"
                TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE>w ʕaṭēt-u ad-den āni bidāl haḏāk w fakkēt šarr-hum miššān aḷḷa. hāḏi axēr min ačil al-Ḥasan w-al-ᵊḤsēn.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25"
                TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>ǧāʕid uxṛa guṭma haḏōl uxṛa ytimaʕmaʕūn: maʕmaʕmaʕ. ǧūʕānīn! al-ǧūʕān al-wlēd al-ʕaǧi aǧ-ǧūʕān ši-ysāwi? yibčiʸ.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26"
                TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE>yṣīḥ: yuṃṃa! āni ǧūʕān arīd xubuz arīd mayye arīd ačil. ačil māmiš b-al-bēt.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
