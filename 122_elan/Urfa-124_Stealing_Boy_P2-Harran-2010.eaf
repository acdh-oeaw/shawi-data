        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>“ā!” gāyil “yā bāše iḏbaḥū-ni hīčiḏ miṯil-ma ḏibaḥit ad-diǧāǧe!”</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>ē al-bāše mā yigdar yḥuṭṭ iṯm-u b-kellt-u, čibīre kellt-u! “wal!” gāyil “hīčiḏ āni mā_gdar aḏbaḥ-ak, hīčiḏ māmiš ḏabiḥ. xayyo ʕaff ʕinn-ak, hīčiḏ mā_gdar aḏbaḥ-ak.”</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
