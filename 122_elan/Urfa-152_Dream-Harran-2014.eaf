    <TIER LINGUISTIC_TYPE_REF="default-lt" TIER_ID="default">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>b-al-lēl šifit ḥilm-in zēn yaʕni ḥlimit b-al-lēl ḥilim, ḥlimit inn-i b-bēt Aḥmad hāḏa, axu ʕAli, baǧanāqi, axū-´ akbar minn-u, Aḥmad, iḥne nsammī-´ Aḥmad Güčtakin, Aḥmad, ybīʕūn ǧigār wēlād-u, ǧiger kebabı.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>ᵊḥlimit inn-i b-bēt-u, w-inn-i b-bēt-u ṣubbut ḥunṭa, amma šnōn-he al-ḥunṭa! ᵊšgadd zēne! </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>ḥabbit-he ᵊtgūl ṣūwān hīčiḏ ḥunṭut-in zēne ᵊhbalat-ni yaʕni min-gadd mī zēne al-ḥunṭa.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>winn-u wāǧfīn huwwa, Aḥmad w zawǧit-u, rāʕit bēt-u w-umm-u aḷḷa yirḥam umm-u zād mirtaḥme.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>mətṣannʕīn lābsīn ᵊhdūm-in zēnāt, humma wǧūh-hum zēne ᵊšgadd zēnīn yaʕni beyle.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>yrīdūn ysāfrūn yrīdūn yrūḥūn ʕala darib, winn-i āni, ʕid-hum adaḥḥiǧ ʕa-l-ḥunṭa timaḥḥanit ʕala zēn al-ḥunṭa.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>win yamm al-ḥunṭa ᵊhniyye Maʕrūf ǧaʕad yṣalli, ǧār-na Maʕrūf, Maʕrūf gaḷb-u naḏ̣īf, yṣalli ṣalāt-in zēne maqbūḷa.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>win min qādi zād ǧaʕad yṣalli Xalīl, berber Xallē, iḥna nsammī-´ berber, rāʕi dukkān-ne.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>winn-u yṣalli, Xallē, ṣalāt-u tgūl mī maqbūḷa, miṯil ᵊtgūl bī-´ ǧuwwak, ǧuwwak yaʕni hīčiḏ ynām…</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>b-wuǧūd-u hīčiḏ yxammiš ᵊb-ḥāl-u w yṣalli ṣalāt-u mā tinqabil yrīd tā yṣalli mā yigdar yṣalli kull-u yxammiš ᵊb-ḥāl-u</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>Xallē guṭma šwayy mā yigdar l-iṯm-u āni min hāḏa agūl yaʕni čünkü šift-u waḷḷa</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>ʕamann-u yṣalli ṣaḥīḥ al-mislim bass iṯm-u, yōm-in yšūf iṯnēne hemen yḥuṭṭ bēnāt-hum fisād, yifsid bēnāt-hum, yaʕni lsān-u šwayy guṭma bozuk, mā yʕarif ᵊhnīt ᵊlsān-u.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>min hāḏa agūl ṣalāt-u mā tingabil, al yṣalli, al-mislim, lāzim al-mislim ism-u mislim yaʕni, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>in ṣalla al yṣalli lāzim kull šēniyye mā ysāwī. al ysāwi šēniyye ṣalāt-u mā tinfaʕ-u</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>al ysāwi šēniyye ṣalāt-u mā tinfaʕ-u yaʕni ṣalāt-u taʕab, qēr at-taʕab māmiš taʕab</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16"
                TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>w šifit Aḥmad al-ᵊḤḏ̣ariyye aḷḷa yiʕlam, umm-u čiṯīr zēne, winn-i āni zād asōlif ḥāl-i, agūl āni iḥna zād amis ṭaḥanne xamis ʕalāyiǧ ḥunṭa</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17"
                TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>ḥunṭut-ne zġār bass agbal aḷḷa yiʕlam al-ḥunṭa riziq yaʕni inšaḷḷa rabb al-ʕālamīn yšūf al-ḥunṭa,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>čiṯīr yaʕni al-… ḥilm-i ʕǧibt-u, farraḥ-ni, ḥilm-i farraḥ-ni yaʕni… ǧī ʕala xōš-i, zʸēn yaʕni, kayyafit w salāmt-ak w-al-ʕāfye xōǧam.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER LINGUISTIC_TYPE_REF="Translation" PARENT_REF="default" TIER_ID="Translation">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a19" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Tonight, I had a beatiful dream; in the night I had a dream. I dreamt that I was at Aḥmad’s house, ʕAli’s brother, his brother in law. His brother who is older than him. Aḥmad whom we call Aḥmad Güčtakin; Aḥmad whose children sell liver, liver kebabs.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a20" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>I dreamt that I was at his house and that, when I was at his house, there was a heap of wheat, but what sort of wheat! How good!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a21" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>You may say its grains were like flint stones; it was such good wheat that it drove me crazy. I mean because the wheat was so good.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a22" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>They were standing: Aḥmad and his wife, his spouse, and his mother. God bless his mother’s soul, she has passed away.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a23" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>They were well-groomed and dressed in beautiful clothes, their faces were beautiful, how beautiful! Like that.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>They wanted to travel, they wanted to go on a trip and I was at their place looking at the wheat. I was thrilled about the quality of the wheat.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>And next to the wheat Maʕrūf was praying, our neighbor Maʕrūf. Maʕrūf has a good heart and he prays a good prayer that is accepted (by God).</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a26" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>And there was Xalīl also praying. Xalīl the barber, we call him the barber, the landlord of our shop.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>He, Xallē, was praying too, but you may say that his prayer is not accepted. Like you say there is something that causes itching; like this, it sleeps…</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>in his body like that and he scratches himself while praying. His prayer will not be accepted; he wants to pray, but he cannot pray because he always scratches himself.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>Xallē cannot keep his mouth. That is why I am saying that, I mean, because I swear that I saw him.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>Because he is praying correctly according to the Islamic rules, but his mouth… Whenever he sees two people, he immediately creates troubles between them, he creates strife among them, I mean, his tongue is a little bit bad, he does not know the (limit) of his tongue.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>That is why I am saying that his prayer does not get accepted. The one who prays, the Muslim, he must… he is called a Muslim.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>If he prays; the one who prays must not do any bad thing. The one who does a bad thing will not benefit from his prayer.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>The one who does a bad thing will not benefit from his prayer. I mean his prayer is just an effort, but there is nothing in it but getting tired.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>I saw Aḥmad al-ᵊḤḏ̣ariyye, God knows that his mother is very good. I was talking to myself, saying that yesterday we milled five sacks of wheat.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>Our wheat is small, but I take it, God knows, the wheat is our daily bread (provided by God). Hopefully the Lord of the Worlds will see the wheat.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>It was a lot. I really liked my dream, it made me happy, my dream made me happy, I mean, I took pleasure in it, I liked it. Enjoy it!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
