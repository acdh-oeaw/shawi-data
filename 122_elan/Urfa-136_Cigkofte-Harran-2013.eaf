        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>ač-čīgt al-faṛək ta-ngūl al-burqul awwal fāl ᵊnǧīb al-burqul, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>al-burqul šaklēn šī b-burqul, burqul ač-čīge an-nāʕim.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>w dibis-he w ṣōṭ-he, ṣōṭ bēt yrīd yṣīr dibis bēt zād yṣīr ʕalē-he al-ḥāḏ̣ir zād yṣīr al-bēt zād. hā!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>w tgūm tufruk-hin tugruḏ̣ ʕalē-hin ṯalāṯ arbaʕ ᵊsnūn ṣamərsaq, aṯ-ṯūm. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>w-ᵊhnīt bəṣal, rās bəṣal ta-ngūl, ᵊbṣale wəḥde tugruḏ̣ ʕalē-he bṣalt-in yābse w yinfarčin haḏanne.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>yḏ̣all guṭmut guṭmut mayye tḥuṭṭ ʕalē-hin w tufruk-hin tufruk-hin lummun-hin guṭma yistawin w ssāwī-l-hin īdām, īdām bēḏ̣.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>tiksir ʕalē-hin bēḏ̣tēn w-īdām-hin guṭma yṣīr, tugruḏ̣ ʕalē-hin baqdanūs w bəṣal-in yā… xaḏ̣ar.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>haḏanne zād yinḥaṭṭin fōg-ha gabəḷ al-īdām yinxalṭin hāḏi čīgt al-bēḏ̣.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>w-al-laḥam zād ač-čīgt al-laḥam ʕayne, al-habra, nafs əš-šī čīgt al-habra zād ʕayne burqul w dibis-he,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>w ʕayne bəṣal-ha ʕayne ʕayne bass lummun ᵊtgūm tufruk-ha hāḏi b-būz tufruk-ha, ᵊb-būz ē ᵊb-qālib būz.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>bala ǧāfi tā al-mayye yḏ̣all zihīd zihīd yḏ̣all ᵊtmūʕ, zād tinfarič hāḏi tiṣīr guṭma alle ṣṣīr ḥāṛṛa, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>mū miṯil čīgt al-bēḏ̣ yṣīr guṭma acılı yṣīr zād tinfarič ʕugub ʕād tinḥaṭṭ ʕalē-he tindagg ʕala l-habra.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>al-habra tin… ʕād al-mille awwali yduggūn-he ʕa-l-ᵊḥǧaṛa b-al… w šift al-mīǧant az-ziqīre awwali daha ṣṣīr axēr.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>amma alḥaz ṭəlʕat qōlay ar-robōṭāt b-ar-robōṭ yinǧarr al-laḥam yinḥaṭṭ ʕalē-he zād tinfarič ʕayne yešilik-he</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>zād kiḏe miṯil-he miṯil uxut-he bass hāḏi ʕala laḥam ʕād ᵊṣṣīr. kull-hin ᵊhniyye miṯil baʕaḏ̣-hin.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16"
                TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>hinne kull-hin miṯil baʕaḏ̣-hin w hnīt al-ʕadas zād ʕayne nafs aš-šī zād kull-he burqul-he w bəṣal-ha w ṯūm-ha al-yābis hā!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17"
                TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>kull-hin wakt-in ᵊtgūm tinfariǧ yḏ̣all tinḥaṭṭ xāšūgit ᵊhnīt ʕadas šōrabit ʕadas-in ḥāṛṛa tā tiswī-he – tā tiswī-he? – ī!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>aḏ̣all kull-ma bidāl al-mayye aḏ̣all aḥuṭṭ šōraba lummun-he tihni tistawi ṣṣīr zēne miṯil ʕaǧīn,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>agūm ʕād aḥuṭṭ ʕalē-he ayaddim ᵊbṣala īdām ᵊbṣala wala hiyye bēḏ̣a wala ṣinʕa,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>ᵊbṣala īdām-he yinḥaṭṭ ʕād ʕalē-he, hāḏi zād čīgt al-ʕadas, w-al-ᵊhniyye zād ʕayne al-bəṭāṭa.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>al-bəṭāṭa zād hašlani ta-ngūl tinṭabix, tinṭabix zād wakt-in-ha ḥāṛṛa ʕayne zād al-burqul w-ad-dibis w-aš-šī kull-u ʕayne nafs aš-šī.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22"
                TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>w bṣalt-in yābse w-aṯ-ṯūm ʕayne wakt-in ᵊtgūm tinfarik bidāl al-ʕadas tinḥaṭṭ bəṭāṭa,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>al-bəṭāṭa zād tinfarik b-al-bəṭāṭa w īdām-he zādēn zēt w bṣalt-in yābse.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24"
                TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE>hāḏi zād ᵊhnīt al-bəṭāṭa haḏanne ač-čīge arbaʕ ašqāl.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER LINGUISTIC_TYPE_REF="Translation" PARENT_REF="default" TIER_ID="Translation">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>As for the kneaded čīge, let’s say, we first bring the bulgur.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a26" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>There are two types of bulgur: the bulgur for čīge is the fine one.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>(We also need) the (tomato) paste and chilli pepper. Home dried chillis and homemade paste should be used; the one which has already been made at home.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>Then you start kneading it; then you mince three or four cloves of garlic and put them on it.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>Some onions, let’s say, one onion. You grate an onion, a yellow onion; and then all these (ingredients) are kneaded thoroughly.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>You keep pouring slowly slowly water on it while kneading and kneading it until it is done. Then you prepare the hot oil with eggs.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>You break two eggs onto it and then you add the hot oil, but only a little. Then you cut parsley and some green onions onto it.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>Actually these are put on it before adding the oil. Then everything gets mixed and this is the čīge with eggs.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>As for the meat: the meat čīge is actually the same and this is also true for the čīge with minced sirloin. (It is also made) with bulgur and a paste of tomato and chilli pepper.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>It’s the same; also with onion, but when you knead it you have to knead it with ice; you rub it on an ice block.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>If you do not mind, until the water is almost gone; it keeps melting. It is kneaded thoroughly and so (the water) becomes less. It even becomes warm.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>Unlike the čīge with eggs, this one is a little bit spicy. It is also kneaded after the minced sirloin has been put on it. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>In former times the people used make the minced sirloin by beating it on a stone. You saw the small mallet (didn’t you?). In former times it became better.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>But nowadays it has become easier thanks to the food processors. With the food processor the meat gets minced and also the vegetables are grated. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>It is also like that; like its ‘sister’ (i.e., the one with the egg), it just is with meat. All of them are the same.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>They are all the same. And the one with lentils is basically also the same. Actually they are all made with bulgur, onions and garlic; with yellow onions!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>With all of them; while you are kneading it a spoon of hot lentil soup should be added until it is done. – Until it is ready? – Yes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>I keep putting the lentil soup instead of water. I add soup until it is done and it becomes good and like dough.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>Then I put (the following) on it: I roast an onion in hot oil. But only a white onion nothing else. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>Then the oil with the onion will be put on it; this is basically the čīge with lentils; and the other one is actually similar, the one with potatoes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>The potatoes are also boiled. Let’s say, when they are boiled, when they are still hot, they (are added) to the bulgur and the tomato and chilli paste. Everything is the same, it’s the same thing.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>Again (you add) a yellow onion and garlic and start kneading it. But instead of the lentils you put potatoes in it. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>It is kneaded with potatoes together with the hot oil, oil, and a yellow onion.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>This is basically the one with potatoes. These are the čīge dishes, there are four types.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
