        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>I: ˀugbal yā ˀAḥmad b-al-ālūfāt xaḏēt guṭun, waḷḷāhi mā qazziǧ məṣāri ˁalē mā-qazniǧ? ˁam-ann-i ˁind-i ṣudug. – A: ṣaḥīḥ.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>I: ˀaǧi ˀagul-lak “ˁammo! məṣārīy-ak bāčir, yēlon al-guṭun ḏe ˀarūḥ abīˁ-u b-ˁašra nāqəṣ alle ˀaǧīb məṣārīy-ak”</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>M: ˀaṣ-ṣudug yinfaˁ. – I: bass ˁād yōm-inn-i ˀaṣdug, mā ˀaqazniǧ.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>M: ṣaḥīḥ ǧuwābak ǧuwāb-u. – I: mā qaznaǧit ˁammo! M: ˀaz-zimān hāḏa zimān ač-čaḏḏāb.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>I: ˀī, ˀugbal Aḥmad! – A: qaznaǧit šaraf. I: ˀal-ḥamdilla čok šükür sintēn ṯalāṯ sāwēt-hin.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>nahār abū-y w čān yṣiḥ-li w-in ygūl “wlēdi, ˀad-agul-lak šækle.” gilt-illu “yāba gūl!”</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>ygūl “wlēdi, ˀaš-šuġul hāḏa mū šuġul-ne. ᵊtrūḥ, ˀaxāf inn-ak ᵊtrūḥ təṭmaˁ ᵊtrūḥ-illak ᵊb-ḥade ˀal-fabrīqāt.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>tinṭi māl, māḷ al-ˁālam, ˁibād aḷḷa, tinṭi māl gaṛāyb-ne, ˀəṣdigā-ne at tinṭī-hin əṣdigā-ne, ˀəṣdigā-ne yinṭūn-ne.”</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>M: ṣaḥīḥ. – I: ˀal mā-yḥibb-ne mā-yinṭī-ne guṭn-u. M: ṣaḥīḥ</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>w-axāf inn-ak ᵊtrūḥ nahār ssawwī-lak ṭāxəlma b-məṭraḥ.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>M: ᵊtqarag ᵊb-məṭraḥ. – I: ᵊtqarag ᵊb-məṭraḥ w-tuṭmaḥ, w ḏīč as-sāˁa ˀāni mā-gdar agūm min ǧawwa ˀal-ḥimil. ˀabīˁ bēt-i, ˀabīˁ ᵊgwāˁ-i uxra ˀaxāf mā waffi.”</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>M: ṣaḥīḥ ǧuwāb-u. – I: yēlōn ywaffin mā ˁala bāli mā-xāf. – M: sāw ˁala l-uṣūl.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>I: ˀ¬ē ˀamma ˀazēn-ak inn-ak tutruk aš-šuġul hāḏa w taˁālu hēne bīˁu gbāḷ-i bəṣāl, bīˁu baṣāl!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>M: ta-šuf-ak ᵊb-ˁēn-i. – I: ˀī, gāḷ bīˁu bəṣal w-šuġl al-guṭun hāḏa uturkū, wlēd-i! </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>w-čān nugḏ̣ub ˁād w nuturk-u ˁammo ḥaǧǧi b-nōba.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16"
                TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>waḷḷa ˀāni min hēne ta-ngūl min məṭraḥ ašgadd maṭāriḥ an-nōb ˁād al-mille ḥēšā-kum ˁammo. – M: ṣaḥīḥ, ṣaḥīḥ!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17"
                TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>I: ˀaǧi ˁind-u ǧiddām bēt-u kamyūn guṭun, xayyo, bēš al-guṭun hāḏa? xayyo ǧaw inṭō-ni tisˁimye bī.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>mesela halbuki mū mənṭī tisˁimye, ǧāy-u darrāb.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>M: ˁalē-k yqallī. – I: yqallī ˁalay-ye, ˀī ˀāni ṣādiǧ, tāǧir-in ṣādiǧ, ˀar-rasūḷ ˁalē s-salām ygūl uṣdugu!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>yaˁni ˀat-tiǧāṛa b-al-baraka, ˀat-tiǧāṛa b-al-baraka, ˀamma w-ar-rizig b-al-ˁašra tisˁe b-at-tiǧāṛa.  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>A: baraka w-aṣ-ṣudug. – I: ˀamma ṣ-ṣādgīn l-aṣ-ṣādgīn. – M: āh aǧi ygūl inṭā-ni tisˁimye, ˀāni ˀagḏ̣ub-u al-gutun aˁarf-illu M: tisˁimye hāḏa mā-yrabbiḥ-ni bass.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22"
                TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>I: mesela ˀagḏ̣ub-u, ˀal-guṭun randemān-u ˀaˁarfu b-īd-i. – A: ši-ygūl. – I: lā, lā b-īd-u, min arukk-u b-īd-i [al-lāzig].</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>bir şifrem var, ˀagūl hāḏa otuz yedi mā-yfūt ˁal-otuz yedi randemān.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
