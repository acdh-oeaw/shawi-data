        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>Namrūd ibin Kanʕān, mū hīčiḏ Ibrāhīm? Namrūd ibin Kanʕān.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>mū ydaḥḥiǧ b-al-… awwali ʕid-hum rammāla, raməl. yfukk al-ᵊktāb ydaḥḥiǧ w yrīd yiḏ̣hir ṭifil, ybaṭṭil dīn-ak.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>gāḷaw il-Namrūd hīčiḏēne, w gām an-nise mā ʕād yxallī-hin yitḥammalin, al-ʕaǧī al yṣīr yiḏbaḥ-u w-al-ḥurumt al-ᵊhniyye al-ḥāmle yšugg baṭin-he.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>Umūs, Umūs, abu Ibrāhīm al-Xalīl, ḥurumt-u ḏ̣allat hīčiḏ, maḥḥad mā … mū balliyye ʕin-he, ḥāmile mū balliyye.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>ǧā-ha wakt-il waḏ̣iʕ, ᵊxriǧat min al-balad w rāḥat ʕa-l-məqāṛa wḏ̣aʕat al-walad hināk. trikatt-u baʕad sāʕtēn aw ṯalāṯ ᵊrǧaʕat ᵊtšūf mayyit ṭayyib, walad!.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>ǧat in fōg-u qazāle tirḏ̣aʕ-u, min aš-šiǧǧ. ᵊtgūlūn l-aš-šiǧǧ? hāḏa aš-šiǧǧ. – hāḏa? – dēd. al-ʕarab ᵊb-Sūriyye ygūlūn dēd, iḥne ngul-l-u šiǧǧ.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>w-al-qazāle tirḏ̣aʕ-u, ǧat xaššat ǧawwa yumma! alḥazzēne ᵊtdūs-u b-baṭn-u, yimūt. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>šālatt-u b-məqāṛa ṯāni fōg, al-mawlūd hāḏa, mawlūd Ibrāhīm al-Xalīl ʕalay-hi s-salām.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>šālatt-u b-al-məqārt aṯ-ṯāni fōg, tā al-qazāle mā timši lē hināk mā tigdar ᵊtrūḥ.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>baʕad yōm ǧat inn-u yimši, yimši, baʕad yōm. – Isa: al-qazāle kun_nahāṛ tirḏ̣aʕ-u mū? – Halil: ī, ī!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>lummun ṣār xaməsṭaʕš ʕumr-u, arbaʕṭaʕš, ǧī xašš ᵊb-ḥuḏ̣un Namrūd, abū-´ ygūlū-l-u ʕAzara.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>huwwa mhandis – mühendis – mühendis, abū-´ mühendis ysāwi pūtāt, ᵊtʕarif al-pūt – naʕam, aʕarif – aṣnām – aṣnām – aṣnām. ē, rabb al-ʕālamīn subḥānahu wa-taʕāla ygūl huṃṃa yiṭulʕūn ʕalā məkān, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>al-ʕālam kull-he trūḥ hināk, gāḷ minhu yitni al-kilīse? gālaw: hāḏa xall-u yitni hēne iḥna nrūh.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>gāḷ: tamām rūḥaw intu! hāḏa ǧī  faššax-hin kull-hin, b-al-balṭa, faššax-hin w ḥaṭṭ b-ᵊrgubt-u al-kibīr al-hibil al-kibīr al-ak… kibīr al-aṣnām.</ANNOTATION_VALUE>
